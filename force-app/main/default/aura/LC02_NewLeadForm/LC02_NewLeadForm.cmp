				  			<div class="slds-form-element__control slds-size_1-of-2 date slds-p-right_small">
			  					<lightning:input type="date" timeZone="{! $Locale.timezone }"  class="dateInput"  aura:id="VisitPreferredDate__c" value="{!v.lead.VisitPreferredDate__c}" label="Preferred Date of Visit"  />
				  			</div>
							<div class="slds-form-element__control slds-size_1-of-2 slds-p-left_small">
