    <rules>
        <fullName>ACC_ApplicationFormNotif</fullName>
        <actions>
            <name>AccNotificationEmailOnlineApplicationForm</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND (2 OR 3)</booleanFilter>
        <criteriaItems>
            <field>Account.OnlineApplication__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
