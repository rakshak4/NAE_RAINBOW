    <rules>
        <fullName>Corporate Account</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.Parent_Corporate_Account__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Add in the corporate account</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>North Broward BAM</fullName>
        <actions>
            <name>North_Broward_BAM_Alert_for_Opportunities</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 OR 2) AND 3</booleanFilter>
        <criteriaItems>
            <field>Opportunity.Name_s_of_Other_Schools_Interested_in__c</field>
            <operation>contains</operation>
            <value>North Broward</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Preferred_School__c</field>
            <operation>contains</operation>
            <value>North Broward</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.DayOrBoardingSchool__c</field>
            <operation>contains</operation>
            <value>Boarding</value>
        </criteriaItems>
        <description>If North Broward school is named as an alternative school email the BAM</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>OPP_ApplicationStartDate</fullName>
        <actions>
            <name>OPP_UpdateApplicationStartDate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND( 		NOT($Setup.CS_Bypass__c.BypassValidationRules__c), 		ISPICKVAL(StageName,&apos;Application&apos;) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>OPP_DeferredStartDate</fullName>
        <actions>
            <name>OPP_UpdateDeferredStartDate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Auto populate &apos;deferred start date&apos; when the Opportunity stage is selected as &quot;Deferred&quot;</description>
        <formula>AND( 		NOT($Setup.CS_Bypass__c.BypassValidationRules__c), 		ISPICKVAL(StageName,&apos;Deferred&apos;) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>OPP_SendOwnerNotificationStarted</fullName>
        <actions>
            <name>OPP_SendOwnerNotificationStarted</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <formula>AND(  NOT($Setup.CS_Bypass__c.BypassWorkflows__c),  ISPICKVAL(StageName ,&apos;Started&apos;) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Opp - Add Email address from School</fullName>
        <actions>
            <name>Admissions_Officer_Central_Email</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.Name</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Add the admissions email address to all the opportunities - used for the central email trigger for notification</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Opp_StartDateChangeStageEnrol</fullName>
        <actions>
            <name>Opp_startdate_change_or_stage_enrolled</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Fires every time the starting date of a opportunity is changed or when an opportunity is changed to the enrolled stage in SF.</description>
        <formula>AND ( SchoolOfInterest__r.SchoolCode__c = &apos;BCB&apos;,
OR (ISCHANGED(CloseDate),
AND(ISCHANGED(StageName)) &amp;&amp; ISPICKVAL(StageName,&quot;Enrolled&quot;)))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Opportunity Assessment Done</fullName>
        <actions>
            <name>OPP_UpdateAssessmentCompletedDate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>This fires when an assessment is done</description>
        <formula>AND(NOT( $Setup.CS_Bypass__c.BypassWorkflows__c ), AssessmentDone__c )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Update to Enrolled Stage</fullName>
        <actions>
            <name>Change_to_Enrolled</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>Opportunity.Deposit_Received__c</field>
            <operation>equals</operation>
            <value>Deposit Form</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Deposit_Received__c</field>
            <operation>equals</operation>
            <value>External Payment Confirmed</value>
        </criteriaItems>
        <description>Update to enrolled stage when the record deposit field is on External payment confirmed</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
