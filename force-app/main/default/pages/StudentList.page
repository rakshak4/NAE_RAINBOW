<apex:page standardController="Account" extensions="StudentListController" showHeader="false" sidebar="false">
 <apex:pageBlock > 
    <apex:pageBlockTable value="{!studentList}" var="a">
        <apex:column >
            <apex:facet name="header">{!$ObjectType.Contact.Fields.Student_Name__c.Label}</apex:facet>
            <apex:outputPanel >
                <apex:outputLink target="_parent" value="/{!a.Id}">{!a.Student_Name__c}</apex:outputLink>
            </apex:outputPanel>            
        </apex:column>
        <apex:column >
             <apex:facet name="header">{!$ObjectType.Contact.Fields.Stage__c.Label}</apex:facet>
             <apex:outputField value="{!a.Stage__c}"/>
         </apex:column>
        <apex:column >
             <apex:facet name="header">{!$ObjectType.Contact.Fields.Class_Name__c.Label}</apex:facet>
             <apex:outputField value="{!a.Class_Name__c}"/>
         </apex:column>
         <apex:column >
             <apex:facet name="header">{!$ObjectType.Contact.Fields.House__c.Label}</apex:facet>
             <apex:outputField value="{!a.House__c}"/>
         </apex:column>
         <apex:column >
             <apex:facet name="header">{!$ObjectType.Contact.Fields.Student_Email_Formula__c.Label}</apex:facet>
             <apex:outputField value="{!a.Student_Email_Formula__c}"/>
         </apex:column>
    </apex:pageBlockTable>
  </apex:pageBlock> 
</apex:page>