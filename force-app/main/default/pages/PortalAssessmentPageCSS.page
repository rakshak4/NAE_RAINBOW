/* Conduct card */

.conduct-summary{
    font-family: "Sans Pro";
    list-style-type: none;
    list-style-image: none;
    margin: 18px 0 45px 0;
    padding: 0; 
    height: 51px;
}

.conduct-summary li{
    float: left;
    text-align: right;
    margin-right: 8px;
    padding: 0;
    width: 20%;
    line-height: 19px;
    color: #999999;
    position: relative;
    right: 8px;
}
/* Theming the color blocks only for the attendance report */
.conduct-summary .attendance{
    float: left;
    text-align: right;
    margin-right: 8px;
    padding: 0;
    width: 16%;
    line-height: 19px;
    color: #999999;
    position: relative;
    right: 8px;
}
.conduct-summary .attendance .cs-value-title {
  float: right;
  width: 16%;
  position: absolute;
  top: 55px;
  margin: 0 0 0 10px;
  line-height: 1.2em;
}

.conduct-summary li.disabled{
    color: #D5D5D5;
}

.conduct-summary li .cs-value{
    border: solid 1px #CCCCCC;
    font-size: 20px;
    height: 51px;
    width: 51px;
    display: block;
    float: right;
    text-align: center;
    margin: 0 0 0 10px;
    line-height: 51px;
    color: #FFFFFF;
}

.conduct-summary li.disabled .cs-value{
    background-color: #D5D5D5 !important;
}

.conduct-summary li.cs-first .cs-value{
    background-color: #FDC02D;
}

.conduct-summary li.cs-second .cs-value{
    background-color: #EA3056;
}

.conduct-summary li.cs-third .cs-value{
    background-color: #F28650;
}

.conduct-summary li.cs-fourth .cs-value{
    background-color: #722C8A;
}

/* Forgot Password */
.fp-container, .login-container, .logout-container{
    width: 285px;
    font-family: "Sans Pro";
}

.fp-container h1, .login-container h1, .logout-container h1{
    color: #003255;
    font-size: 30px;
    font-weight: normal;
}

.fp-container .box, .login-container .box, .logout-container .box{
    border: solid 1px #CCCCCC;
    padding: 7px 9px 11px 9px;
    margin: 0 0 20px 0;
}

.fp-container .box::after, .login-container .box::after, .logout-container .box::after{
    content: '';
    background-image: url("{!URLFOR($Resource.ParentPortalFiles,'fp-arrow.png')}");
    height: 19px;
    width: 31px;
    display: block;
    position: absolute;
    margin: 11px 0 0 14px;
}

.fp-container .desc, .login-container .desc{
    color: #999999;
    font-size: 16px;
    margin: 3px 0 0 0;
    display: block;
}

.fp-container .input, .login-container .input, .login-container .input-pass{
    margin: 5px 0 0 0;
    padding: 10px 0 10px 9px;
    height: 45px;
    color: #FFE3B1;
    background-color: #003255;
    font-size: 20pt;
    line-height: 28px;
    border: 0 none;
    width: 255px;
}

.fp-container .submit, .login-container .submit, .logout-container .submit{
    height: 21px;
    background-color: #003255;
    background-image: none;
    color: #FFFFFF;
    padding: 13px 0 12px 0;
    margin-top: 3px;
    line-height: 23px;
    font-size: 23px;
    border: 0 none;
    min-width: 65%;
    height: 100%;
    text-transform: uppercase;
    cursor: pointer;
}

.fp-container .retun, .login-container .forgot-pass{
    color: #003255;
    margin: 5px 0 0 0;
}

