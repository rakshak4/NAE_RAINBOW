<apex:page standardStylesheets="false" sidebar="false" showHeader="false" contentType="text/css">
/* Report card */
/* General styles */

@font-face {
font-family: "Sans Pro";
src: url('{!URLFOR($Resource.ParentPortalFiles, 'source-sans-pro.regular.ttf')}');
}

@font-face {
font-family: "Dear Joe 5";
src: url('{!URLFOR($Resource.ParentPortalFiles, 'DJ5CAS_P.ttf')}');
}

body{
    margin: 0 auto;
    padding: 0px 0;
}
p {
  font-family: "Sans Pro";
  line-height: 1.3em;
}

h1, h2, h3{
    font-family: "Sans Pro";
    margin: 0;
    padding: 0;
    line-height: 1.3em;
}

.clearfix{
    clear: both;
}

/* Sidebar */

#sidebar{
    width: 240px;
    min-height: 300px;
    float: left;
    display: block;
}

#sidebar .sidebar-top{
    color: #003255;
    min-height: 120px;
    border: solid 1px #CCCCCC;
    display: block;
    padding: 5px 20px 25px 10px;
    position: relative;
    display: block;
}

#sidebar .sidebar-top h1{
    font-family: "Dear Joe 5";
    font-size: 30px;
    font-weight: normal;
}

#sidebar .sidebar-top h3{
    bottom: 0;
    position: absolute;
    margin: 0 0 10px 0;
    font-size: 14px;
    font-weight: normal;
}

#sidebar .sidebar-top:after{
    width: 32px;
    height: 19px;
    content: '';
    background-image: url("{!URLFOR($Resource.ParentPortalFiles,'sidebar-top.png')}");
    display: block;
    position: absolute;
    bottom: 0;
    margin: 0 0 -19px 5px;
}

#sidebar .section.first{
    margin: 25px 0 0 0;
}

#sidebar .section{
    border: solid 1px #CCCCCC;
    padding: 9px 6px 6px 6px;
    margin: 5px 0 0 0;
    display: block;
}

#sidebar .section .section-heading{
    font-family: "Sans Pro";
    color: #666666;
    font-size: 10px;
    font-weight: normal;
}

#sidebar .btn-section{
    font-family: "Sans Pro";
    height: 21px;
    background-color: #003255;
    background-image: none;
    color: #FFFFFF;
    padding: 13px 0 12px 0;
    margin-top: 3px;
    line-height: 23px;
    font-size: 23px;
    border: 0 none;
    width: 100%;
    height: 100%;
    font-weight: normal;
}

#sidebar .btn-section.select{
    width: auto !important;
    padding-left: 8px;
}

#sidebar .button{
    font-family: "Sans Pro";
    text-transform: uppercase;
    text-align: center;
    cursor: pointer;
}

#sidebar .section-select{
    font-family: "Sans Pro";
    background: url("{!URLFOR($Resource.ParentPortalFiles,'section-arrow.png')}") 184px no-repeat #003255;
    overflow: hidden;
    height: 41px;
    margin: 3px 0 0 0;
}

