#sidebar .section-select select{
    font-family: "Sans Pro";
    background: transparent;
    border: 0 none;
    border-radius: 0;
    width: 100%;
    font-size: 23px;
    height: 41px;
    line-height: 27px;
    padding: 4px 0px 7px 7px;
    color: #FFFFFF;
    font-size: 23px;
    width: 243px;
    -webkit-appearance: none;
    /* used for too long names on the select box */
    width: 226px !important;
    overflow: hidden;
    text-overflow:hidden !important;
    padding-right: 55px;
    /* this is to hide the select icon in FF and IE */
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    text-indent: 1px;
    text-overflow: ' ';
}
select::-ms-expand {
    display: none;
}

#sidebar .section-select select:hover{
    background-color: #003255;
    background: url("{!URLFOR($Resource.ParentPortalFiles,'section-arrow.png')}") 184px no-repeat #003255;
}

#sidebar .section-select select option{
    font-family: "Sans Pro";
    background-color: #003255;
}

#sidebar .section .chk-container{
    margin: 15px 0 0 0;
}

#sidebar .section .chk-container td.first{
    padding: 0;
}

#sidebar .section .chk-container td{
    padding: 27px 0 0 0;
}

#sidebar .section .chekbox{
    margin: 0;
    padding: 0;
    display: none;
}

#sidebar .section .chekbox + .chk-display{
    content: '';
    display: block;
    border: solid 2px #003255;
    width: 12px;
    height: 12px;
    float: left;
    margin-top: 4px;
    border-radius: 2px;
}

#sidebar .section .chekbox:checked + .chk-display{
    content: '';
    display: block;
    border: solid 2px #003255;
    width: 12px;
    height: 12px;
    float: left;
    margin-top: 4px;
    border-radius: 2px;
}

#sidebar .section .chekbox:checked + .chk-display .fill{
    display: block;
    width: 13px;
    height: 11px;
    background-image: url("{!URLFOR($Resource.ParentPortalFiles,'chk-tick.png')}");
}

#sidebar .section .chk-label{
    font-family: "Sans Pro";
    color: #003255;
    display: block;
    font-size: 14px;
    height: 16px;
    line-height: 14px;
    margin-top: 4px;
    padding: 0 0 0 20px;
    position: relative;
    width: 120%;
    z-index: 100;
}

#sidebar .section .chk-container .chk-side{
    border: solid 1px #CCCCCC;
    width: 10px;
    height: 32px;
    position: absolute;
    margin-left: -20px;
    margin-top: -4px;
}

#sidebar .section .chk-container .chk-side.chk1{
    background-color: #FDC02D;
}

#sidebar .section .chk-container .chk-side.chk2{
    background-color: #EA3056;
}

#sidebar .section .chk-container .chk-side.chk3{
    background-color: #F28650;
}

#sidebar .section .chk-container .chk-side.chk4{
    background-color: #722C8A;
}

#sidebar .btn-print{
    background-image: url("{!URLFOR($Resource.ParentPortalFiles,'print-icon.png')}");
    font-family: "Sans Pro";
    background-repeat: no-repeat;
    background-position: 60px 50%;
    padding-left: 40px;
    width: auto !important;
    display: block;
    text-decoration: none;
}

/* Main content */

#main{
    font-family: "Sans Pro";
    padding: 0 0 0 25px;
    margin: 0 0 0 0px;
    width: 670px;
}

#main #header{
