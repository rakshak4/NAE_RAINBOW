    <validationRules>
        <fullName>Approve_Only_Grants_Subject_to_Review</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(Grant_Application__r.Scope_of_Grant__c)=&quot;Indefinite&quot;,TEXT(Decision__c)=&quot;Approve Extension&quot;)</errorConditionFormula>
        <errorMessage>The Scope of the Grant has been set as &quot;Indefinite&quot; without needing a review. No Approval is required. Change the scope field in the Application record, if you would like to introduce a review cycle.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Required_for_Rejected_Reviews</fullName>
        <active>true</active>
        <description>Ensure that an end date is added for a rejected review so that the financial aid application shows the correct status</description>
        <errorConditionFormula>AND(TEXT(Decision__c) = &quot;Reject&quot;, TEXT(End_Date_If_Rejected__c) = &quot;&quot;)</errorConditionFormula>
        <errorMessage>As this review has been rejected you must indicate the date on which the financial aid will end. Typically this is the 31/07 of the relevant academic year.</errorMessage>
    </validationRules>
