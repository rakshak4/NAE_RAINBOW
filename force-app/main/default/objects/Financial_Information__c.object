    <validationRules>
        <fullName>Ensure_Shared_Desc_with_Shared_Fees</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    OR(
       INCLUDES(Fees_Responsible_For__c, &quot;All Applicable (Shared)&quot;),
       INCLUDES(Fees_Responsible_For__c, &quot;Tuition (Shared)&quot;),
       INCLUDES(Fees_Responsible_For__c, &quot;Lunch (Shared)&quot;),
       INCLUDES(Fees_Responsible_For__c, &quot;Uniform (Shared)&quot;),
       INCLUDES(Fees_Responsible_For__c, &quot;Bus (Shared)&quot;),
       INCLUDES(Fees_Responsible_For__c, &quot;EAL / Language Support (Shared)&quot;),
       INCLUDES(Fees_Responsible_For__c, &quot;Extra Programmes (ASAs etc.) (Shared)&quot;),
       INCLUDES(Fees_Responsible_For__c, &quot;Fee Required For Enrolment (Shared)&quot;),
       INCLUDES(Fees_Responsible_For__c, &quot;Annual Enrolment (Shared)&quot;),
       INCLUDES(Fees_Responsible_For__c, &quot;Annual Capital Contribution (Shared)&quot;)
       ),
     ISBLANK(Shared_Fee_Breakdown_Details__c)
     )</errorConditionFormula>
        <errorMessage>You have indicated that some of the fees are to be shared so please provide details in the &apos;Shared Fee Breakdown/Details&apos; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Shared_Fees_with_Shared_Desc</fullName>
