    <fields>
        <fullName>Academic_Year_Dashboard_Filter__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   OR(
      AND(
          First_Day_of_Start_Year__c &gt;= $User.Start_Date_Next_Fiscal_Year__c,
          First_Day_of_Start_Year__c &lt;= $User.End_Date_Next_Fiscal_Year__c
          ),
      AND(
          TEXT(Scope_of_Grant__c) != &quot;Indefinite subject to Review&quot;,
          OR(
             ISBLANK(End_Date__c),
             End_Date__c &gt;= $User.Start_Date_Next_Fiscal_Year__c
             )
          )
      ),
   &quot;Next Year;&quot;,
   &quot;&quot;
   ) &amp;

IF(
   AND(
       First_Day_of_Start_Year__c &gt;= $User.Start_Date_This_Fiscal_Year__c,
       First_Day_of_Start_Year__c &lt;= $User.End_Date_This_Fiscal_Year__c
       ),
   &quot;This Year;&quot;,
   &quot;&quot;
   )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
