    <fields>
        <fullName>Decision_Formula__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   OR(
      AND(
          End_Date__c &lt; TODAY(),
          TEXT(Decision_Text__c) = &quot;Approved&quot;
          ),
      AND( 
          TODAY() &gt;= DATE(YEAR(Next_Review_Date2__c),08,01),
          TEXT(Scope_of_Grant__c) = &quot;Indefinite subject to Review&quot;,
          NOT(ISBLANK(Start_Year__c)),
          TEXT(Decision_Text__c) = &quot;Approved&quot;
          )
      ),
   &quot;Financial Aid Ended&quot;,
   IF( 
      AND(
          TODAY() &lt; DATE(YEAR(Next_Review_Date2__c),08,01),
          TEXT(Scope_of_Grant__c) = &quot;Indefinite subject to Review&quot;,
          NOT(ISBLANK(Start_Year__c)),
          TEXT(Decision_Text__c) = &quot;Approved&quot;
          ),   
      &quot;Approved, Ending on &quot;&amp; TEXT(Ending_On_Date__c), 
      Text(Decision_Text__c)
      )
   )</formula>
