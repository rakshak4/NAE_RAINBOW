    <validationRules>
        <fullName>End_Date_for_Apps_With_Reviews</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
    /*End_Date__c &gt; DATE(VALUE(LEFT(Start_Year__c, 4))+1,7,31), THIS LINE WILL ONLY WORK FOR APPS WITHOUT THE OLD REVIEWS */
    End_Date__c &gt; DATE(YEAR(PRIORVALUE(Next_Review_Date2__c)),7,31),
    TEXT(Scope_of_Grant__c) = &quot;Indefinite subject to Review&quot;
    )</errorConditionFormula>
        <errorMessage>The end date for this financial aid must be before the end of the academic year in which the next review is due i.e. on or before 31 July</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_with_Specific_End_Date_Scope</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TEXT(End_Date__c) =&quot;&quot;, TEXT(Scope_of_Grant__c)=&quot;Specific End Date&quot;)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>You must enter an end date if you select Scope=&quot;Specific End Date&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Change_of_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    $Profile.Id != &quot;00e6F000001hU3F&quot;,
    Record_is_Locked__c = TRUE,
    TEXT(Scope_of_Grant__c) = &quot;Specific End Date&quot;,
    ISCHANGED(End_Date__c)
    )</errorConditionFormula>
        <errorMessage>You cannot change the End Date of a financial aid record with &apos;Scope of Grant&apos; set to &apos;Specific End Date&apos; if it has already been approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_End_Date_Change_for_Apps_With_Reviews</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
    ISCHANGED(End_Date__c),
    NOT(ISBLANK(End_Date__c)),
    NOT(ISBLANK(PRIORVALUE(End_Date__c))),
    TEXT(Scope_of_Grant__c) = &quot;Indefinite subject to Review&quot;
    )</errorConditionFormula>
        <errorMessage>You cannot directly change the end date for this type of financial aid. Please delete the end date first, save and then re-enter.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Temp_Prevent_Start_Term_Year_Change</fullName>
        <active>true</active>
        <description>A temporary validation rule in lieu of making the start term and start year fields read only once the data has been added for all approved/locked financial aid records</description>
        <errorConditionFormula>AND(
    $Profile.Name != &quot;System Administrator&quot;,
    OR(
       RecordType.DeveloperName = &quot;Financial_Aid_Review_Locked&quot;,
       RecordType.DeveloperName = &quot;Financial_Aid_Application_Locked&quot;
       ),
    OR(
       AND(
           NOT(ISPICKVAL(PRIORVALUE(Start_Term__c), &quot;&quot;)),
           ISBLANK(TEXT(Start_Term__c))
           ),
       AND(
           NOT(ISPICKVAL(PRIORVALUE(Start_Term__c), &quot;&quot;)),
           ISCHANGED(Start_Term__c)
           ),
       AND(
           NOT(ISBLANK(PRIORVALUE(Start_Year__c))),
           ISBLANK(Start_Year__c)
           ),
       AND(
           NOT(ISBLANK(PRIORVALUE(Start_Year__c))),
           ISCHANGED(Start_Year__c)
           )
       )
    )</errorConditionFormula>
        <errorMessage>You cannot change or remove the start term or year from a locked record. Those fields are only currently available to allow values to be added when the fields are empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Start_Year</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    NOT(ISBLANK(Start_Year__c)),
    OR(
       NOT(ISNUMBER(LEFT(Start_Year__c, 4))),
       NOT(ISNUMBER(RIGHT(Start_Year__c, 2))),
       MID(Start_Year__c, 5, 1) != &quot;/&quot;,
       LEN(Start_Year__c) != 7,
       VALUE(RIGHT(Start_Year__c, 2)) != VALUE(MID(Start_Year__c, 3, 2)) + 1
       )
    )</errorConditionFormula>
        <errorMessage>The start year must be in the format YYYY/YY e.g. 2016/17 and there must only be one year difference between the years e.g. 2016/18 is not accepted.</errorMessage>
    </validationRules>
