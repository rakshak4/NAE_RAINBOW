        <fullName>Translation_Complete</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Translation Complete</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}
if(&quot;{!Web_Content__c.Translation_Status__c}&quot; != &quot;Requested&quot;){
	alert(&quot;You cannot click the &apos;Translation Complete&apos; button if a translation has not been requested.&quot;);
}else{
	var confirmResult = window.confirm(&quot;By clicking this button you are confirming that you are now happy for the content to be displayed on the school website and you may no longer have permission to edit it. If you wish to make further edits then please click &apos;Cancel&apos;. Do you wish to continue?&quot;);
	if(confirmResult){
		var records = [];
		var record = new sforce.SObject(&quot;Web_Content__c&quot;);
		record.Id = &quot;{!Web_Content__c.Id}&quot;;
		record.Translation_Complete__c = true;
                record.Last_Translation_Completed__c = new Date().toISOString();
                record.Last_Translation_Completed_By__c = &quot;{!$User.FirstName} {!$User.LastName}&quot;;
		records.push(record);
		var result = sforce.connection.update(records);
		//alert(&quot;2:result=&quot;+result);
		var sb = &quot;&quot;;
		for (var i=0; i&lt;result.length; i++) {
			if (result[i].getBoolean(&quot;success&quot;)) {
			//sb += &quot;\n Success: Translation marked as completed.&quot;;
			} else {
				sb += &quot;\n Error. &quot; + result[i];
			}
		}
		if(sb != &quot;&quot;){
			alert(sb);
			
		}
		location.reload();	

	}
}</url>
