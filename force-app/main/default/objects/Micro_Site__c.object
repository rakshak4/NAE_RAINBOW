    <validationRules>
        <fullName>Disallow_Teacher_Change_To_Positioning</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Name), $Profile.Name = &quot;Teacher&quot;)</errorConditionFormula>
        <errorMessage>You cannot change the &apos;Positioning&apos; value of this record. Please click &apos;Cancel&apos; or revert the value in the &apos;Positioning&apos; field to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_Teacher_Creating_Other_Staffs</fullName>
        <active>true</active>
        <description>Stops a teacher creating a staff directory record for someone that is not themselves</description>
        <errorConditionFormula>AND($Profile.Name = &quot;Teacher&quot;, Staff_Member__r.PersonContact.User__r.Id &lt;&gt; $User.Id)</errorConditionFormula>
        <errorMessage>You cannot create or edit a staff directory record for another staff member</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>first_record_in_english</fullName>
        <active>true</active>
        <description>Avoid a record with empty English_Parent__c and other language than english</description>
        <errorConditionFormula>AND((Publish_on_School_Site__c = TRUE), ISBLANK(English_Parent__c), OR((ISBLANK(TEXT(Language__c))), NOT(ISPICKVAL(Language__c, &quot;EN&quot;))))</errorConditionFormula>
        <errorDisplayField>Language__c</errorDisplayField>
        <errorMessage>If you want to publish on a School Website in a language different than English (EN) you must first create it in English and then translate it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>first_record_in_english_2</fullName>
        <active>true</active>
        <description>Avoid user create a first record in english (EN) with a value in English_Parent__C field.</description>
        <errorConditionFormula>NOT(ISBLANK(English_Parent__c)) &amp;&amp; ISPICKVAL(Language__c, &quot;EN&quot;)</errorConditionFormula>
        <errorDisplayField>Language__c</errorDisplayField>
        <errorMessage>You are trying to save a translation of a record in english so the language must be other than EN (English).</errorMessage>
    </validationRules>
