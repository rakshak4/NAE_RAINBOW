TEXT(FLOOR(Xero_Value__c / 1000000)) &amp; &quot;,&quot;, 
&quot;&quot;) &amp; 
IF( 
Xero_Value__c &gt;= 1000, 
RIGHT(TEXT(FLOOR(Xero_Value__c / 1000)), 3) &amp; &quot;,&quot;, 
&quot;&quot;) &amp; 
RIGHT(TEXT(FLOOR(Xero_Value__c)), 3) &amp; &quot;.&quot; &amp; 
IF( 
MOD(Xero_Value__c , 1) * 100 &lt; 10, 
&quot;0&quot; &amp; TEXT(ROUND(MOD(Xero_Value__c , 1), 2) * 100), 
TEXT(MIN(ROUND(MOD(Xero_Value__c , 1), 2) * 100, 99)) 
)</formula>
