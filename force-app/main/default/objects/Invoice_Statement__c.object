    <fields>
        <fullName>First_Date_of_Relevant_Term__c</fullName>
        <description>Identifies the start date of the term or year which the invoice applies to, based on the invoice date i.e.
01/08 to 14/12 = 01/08 
15/12 to 14/03 = 01/01
15/03 to 31/07 = 01/04</description>
        <externalId>false</externalId>
        <formula>IF(
   OR(
      AND(
          MONTH(Invoice_Date__c)&gt;=8, 
          MONTH(Invoice_Date__c)&lt;=11
          ),
      AND(MONTH(Invoice_Date__c)=12,
          DAY(Invoice_Date__c)&lt;=14
          )
      ),
   DATE(YEAR(Invoice_Date__c), 8, 1),

IF(
   OR(
      AND(MONTH(Invoice_Date__c)&gt;=1,
          MONTH(Invoice_Date__c)&lt;=2
          ),
      AND(MONTH(Invoice_Date__c)=3,
          DAY(Invoice_Date__c)&lt;=14
          )
      ),
   DATE(YEAR(Invoice_Date__c), 1, 1),
 
IF(
   AND(
       MONTH(Invoice_Date__c)=12, 
       DAY(Invoice_Date__c)&gt;=15
       ),
   DATE(YEAR(Invoice_Date__c)+1, 1, 1),

IF(
   OR(
      AND(MONTH(Invoice_Date__c)&gt;=4,
          MONTH(Invoice_Date__c)&lt;=7
          ),
      AND(MONTH(Invoice_Date__c)=3,
          DAY(Invoice_Date__c)&gt;=15
          )
      ),
   DATE(YEAR(Invoice_Date__c), 4, 1)

   , null))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>First Date of Relevant Term</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
