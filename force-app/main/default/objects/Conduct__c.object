        <externalId>false</externalId>
        <formula>IF(AND(Pastoral_Date__c &gt;= Student_Group__r.Start_Date_Last_Week__c + 7, 
       Pastoral_Date__c &lt;= Student_Group__r.Start_Date_Last_Week__c + 13
       ),
   &quot;This Calendar Week (Mon-Sun) &quot;, &quot;&quot;) &amp;

IF(MONTH(Pastoral_Date__c) = MONTH(TODAY()), &quot;This Calendar Month (1st - Last) &quot;, &quot;&quot;) &amp;

IF(
   AND(Pastoral_Date__c &gt;= Student_Group__r.Start_Date_Last_Week__c, 
       Pastoral_Date__c &lt;= Student_Group__r.Start_Date_Last_Week__c + 6
       ),
   &quot;Last Calendar Week (Mon-Sun) &quot;, &quot;&quot;) &amp;

IF(
   AND(Pastoral_Date__c &gt;= Student_Group__r.Start_Date_Last_Week__c - 7, 
       Pastoral_Date__c &lt;= Student_Group__r.Start_Date_Last_Week__c - 1
       ),
   &quot;Last 2 Calendar Weeks (Mon-Sun) &quot;, &quot;&quot;) &amp;

IF(
   AND(Pastoral_Date__c &gt;= Student_Group__r.Start_Date_Last_Week__c - 21, 
       Pastoral_Date__c &lt;= Student_Group__r.Start_Date_Last_Week__c - 8
       ),
   &quot;Last 4 Calendar Weeks (Mon-Sun) &quot;, &quot;&quot;) &amp;

IF(MONTH(Pastoral_Date__c) = MONTH(TODAY())-1, &quot;Last Calendar Month (1st - Last) &quot;, &quot;&quot;) &amp;

Academic_Year__c &amp; &quot; &quot; &amp; Term__c</formula>
