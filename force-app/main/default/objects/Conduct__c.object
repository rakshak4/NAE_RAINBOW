    <validationRules>
        <fullName>Pastoral_Effort_Attitude_Validation</fullName>
        <active>true</active>
        <description>Pastorals of type &apos;Effort &amp; Attitude&apos; must have a &apos;House Point Value&apos; greater than zero OR &apos;Effort&apos; grade AND a &apos;Pastoral Date&apos; and &apos;Title&apos;.</description>
        <errorConditionFormula>AND(
    RecordType.DeveloperName == &quot;House_Points&quot;,
    OR(
       AND(
           !ISBLANK(TEXT(Effort__c)), 
           !ISBLANK(TEXT(House_Points__c))
           ),
       AND(ISBLANK(TEXT(Effort__c)), 
           OR(ISBLANK(TEXT(House_Points__c)), 
              House_Points__c == 0
              )
           )
       )
    )</errorConditionFormula>
        <errorMessage>Pastorals of type &apos;Effort &amp; Attitude&apos; must have a &apos;House Point Value&apos; OR &apos;Effort&apos; grade (but not both) AND a &apos;Pastoral Date&apos; and &apos;Title&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pastoral_General_Validation</fullName>
        <active>true</active>
        <description>Title and Pastoral Date must be provided, and &apos;Effort&apos; cannot be &quot;------------&quot;</description>
        <errorConditionFormula>AND( 
    RecordType.DeveloperName != &apos;Pastoral_Folder&apos;,
    OR(ISBLANK( Title__c ), 
       ISBLANK( Pastoral_Date__c ), 
       TEXT(Effort__c)=&quot;------------&quot;
       )
    )</errorConditionFormula>
        <errorMessage>Title and Pastoral Date must be provided, and &apos;Effort&apos; cannot be &quot;------------&quot;</errorMessage>
    </validationRules>
