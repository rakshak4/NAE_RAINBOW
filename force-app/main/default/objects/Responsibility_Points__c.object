        <formula>IF(
   Staff_Member__r.Status__c &lt;&gt;&quot;Employed&quot;,
   &quot;Not Active&quot;,
   IF(
      Start_Date__c&gt;Today(),
      &quot;Not Active&quot;,
      IF(
         End_Date__c = NULL,
         &quot;Active&quot;,
          IF(
             AND(
                 End_Date__c &gt;TODAY(),
