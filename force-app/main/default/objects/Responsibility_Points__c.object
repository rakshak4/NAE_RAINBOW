        <formula>IF(TEXT(Review_Date__c)=&quot;&quot;,&quot;&quot;,&quot;Review Date: &quot;&amp;TEXT(DAY(  Review_Date__c  )) + &quot;/&quot; + 
TEXT(MONTH(Review_Date__c)) + &quot;/&quot; + 
RIGHT(TEXT(YEAR(Review_Date__c)),4))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
