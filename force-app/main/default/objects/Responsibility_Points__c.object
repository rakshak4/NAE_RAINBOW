    <fields>
        <fullName>Copy_Text_Cost__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
 Total_Cost__c &gt;= 1000000, 
TEXT(FLOOR( Total_Cost__c  / 1000000)) &amp; &quot;,&quot;, 
&quot;&quot;) &amp; 
IF( 
 Total_Cost__c &gt;= 1000, 
RIGHT(TEXT(FLOOR( Total_Cost__c  / 1000)), 3) &amp; &quot;,&quot;, 
&quot;&quot;) &amp; 
RIGHT(TEXT(FLOOR( Total_Cost__c )), 3) &amp; &quot;.&quot; &amp; 
IF( 
MOD( Total_Cost__c  , 1) * 100 &lt; 10, 
&quot;0&quot; &amp; TEXT(ROUND(MOD( Total_Cost__c  , 1), 2) * 100), 
TEXT(MIN(ROUND(MOD( Total_Cost__c  , 1), 2) * 100, 99)) 
) 

&amp;&quot; &quot;&amp; TEXT(CurrencyIsoCode)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Copy Text Cost</label>
