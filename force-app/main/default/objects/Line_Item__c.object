    <fields>
        <fullName>Audit_Value_Terms_From_Months__c</fullName>
        <description>Calculates the termly equivalent of a monthly fee, based on school, invoice date, starting date and today&apos;s date, for use in the automatic audit reports and dashboard</description>
        <externalId>false</externalId>
        <formula>IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()),  OR(AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=1, MONTH(TODAY())=1), AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=4, MONTH(TODAY())=4), AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=8, MONTH(TODAY())=8, School__c = &quot;The British College of Brazil&quot;), AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=9, OR(MONTH(TODAY())=9, MONTH(TODAY())=8), School__c &lt;&gt; &quot;The British College of Brazil&quot;))), 1,

          IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()), OR(AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=2, MONTH(TODAY())=2), AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=5, MONTH(TODAY())=5, School__c &lt;&gt; &quot;The British College of Brazil&quot;))), 2/3,

          IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()), OR(AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=3, MONTH(TODAY())=3), AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=6, MONTH(TODAY())=6, School__c &lt;&gt; &quot;The British College of Brazil&quot;))), 1/3,
          
          IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()), 
OR(AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=5, MONTH(TODAY())=5, School__c = &quot;The British College of Brazil&quot;), AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=10, MONTH(TODAY())=10, School__c &lt;&gt; &quot;The British College of Brazil&quot;))), 0.75,

          IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()), 
OR(AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=6, MONTH(TODAY())=6, School__c = &quot;The British College of Brazil&quot;), AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=11, MONTH(TODAY())=11, School__c &lt;&gt; &quot;The British College of Brazil&quot;))), 0.5,

          IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()), 
OR(AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=7, MONTH(TODAY())=7, School__c = &quot;The British College of Brazil&quot;), AND(MONTH(Invoice_Statement__r.Invoice_Date__c)=12, MONTH(TODAY())=12, School__c &lt;&gt; &quot;The British College of Brazil&quot;))), 0.25,

          IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()), School__c = &quot;The British College of Brazil&quot;, MONTH(Invoice_Statement__r.Invoice_Date__c)=9, MONTH(TODAY())=9), 0.80,

          IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()), School__c = &quot;The British College of Brazil&quot;, MONTH(Invoice_Statement__r.Invoice_Date__c)=10, MONTH(TODAY())=10), 0.60,

          IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()), School__c = &quot;The British College of Brazil&quot;, MONTH(Invoice_Statement__r.Invoice_Date__c)=11, MONTH(TODAY())=11), 0.40,

          IF(AND(YEAR(Invoice_Statement__r.Invoice_Date__c)=YEAR(TODAY()), School__c = &quot;The British College of Brazil&quot;, MONTH(Invoice_Statement__r.Invoice_Date__c)=12, MONTH(TODAY())=12), 0.20,
          
IF(AND(MONTH(Invoice_Statement__r.Invoice_Date__c)&gt;=8, School__c = &quot;The British College of Brazil&quot;), 0.20, 

IF(OR(AND(MONTH(Invoice_Statement__r.Invoice_Date__c)&gt;=8, School__c &lt;&gt; &quot;The British College of Brazil&quot;), AND(MONTH(Invoice_Statement__r.Invoice_Date__c)&gt;=4, MONTH(Invoice_Statement__r.Invoice_Date__c)&lt;=7, School__c = &quot;The British College of Brazil&quot;)), 0.25, 

1/3))))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Audit Value (Terms From Months)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>4</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
