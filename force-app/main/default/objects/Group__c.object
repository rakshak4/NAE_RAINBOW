        <formula>CASE(TEXT(Combined_Year_Group__c), 
&quot;Pre-Nursery&quot;,&quot;PN&quot;, 
&quot;Nursery&quot;,&quot;N&quot;, 
&quot;Reception&quot;,&quot;R&quot;, 
&quot;Pre-Kindergarten&quot;,&quot;PK&quot;, 
