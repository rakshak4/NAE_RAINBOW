    <fields>
        <fullName>Academic_Year__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   TEXT(Group_Type__c)=&quot;School Group&quot;, 
   Current_Academic_Year__c, 
   IF(
      AND(
          MONTH(DATEVALUE(CreatedDate))&lt;=12, 
          MONTH(DATEVALUE(CreatedDate))&gt;=8
          ), 
      TEXT(YEAR(DATEVALUE(CreatedDate)))&amp;&quot;/&quot;&amp; TEXT(CreatedDate_plus_1__c ),  
      TEXT(CreatedDate_minus_1__c)&amp;&quot;/&quot;&amp;TEXT(YEAR(DATEVALUE(CreatedDate)))
      )
   )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Academic Year</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
