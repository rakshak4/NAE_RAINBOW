    <validationRules>
        <fullName>Disallow_Subject_List_Section_Header</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Subject_Name__c) = &quot;-------- EARLY YEARS ONLY --------&quot;</errorConditionFormula>
        <errorMessage>The Subject Name is Invalid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_Year_Group_Control_Change</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
   ISBLANK(TEXT(Year_Group_Control__c)),
   ISCHANGED(Year_Group_Control__c), 
   AND(
       TEXT(Year_Group_Control__c) = &quot;Tutor Group UK&quot;,  
       $User.Is_US_School_User__c = TRUE
       ), 
   AND(
       TEXT(Year_Group_Control__c) = &quot;Tutor Group US&quot;,
       $User.Is_US_School_User__c = FALSE,
       $Profile.Name != &quot;System Administrator&quot;
       ),
   AND(
       LEFT(TEXT(Year_Group_Control__c), 11) = &quot;Tutor Group&quot;,  
       TEXT(Group_Type__c) != &quot;Tutor Group&quot;
       )
   )</errorConditionFormula>
        <errorMessage>You cannot change the value in the &apos;Year Group Control&apos; field. Please revert the original value or press cancel to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Subject_Group_has_Subject</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(Group_Type__c) = &quot;Subject&quot;, TEXT(Subject_Name__c) = &quot;&quot;)</errorConditionFormula>
        <errorMessage>You must select a subject for a group of type subject group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Tutor_Group_Has_Year_Group</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(Group_Type__c) = &quot;Tutor Group&quot;, TEXT(Year_Group__c) = &quot;&quot;, TEXT(Year_Group_Control__c)&lt;&gt;&quot;Non-Tutor Group&quot;)</errorConditionFormula>
        <errorMessage>You must select a &apos;Year Group&apos; when the &apos;Group Type&apos; is &apos;Tutor Group&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Group_Type_Change_Validation</fullName>
        <active>true</active>
        <description>validates that no group can be update from and to group type &apos;Tutor Group&apos;</description>
        <errorConditionFormula>IF(
AND( ISCHANGED(Group_Type__c),  OR(ISPICKVAL(PRIORVALUE
(Group_Type__c), 
&apos;Tutor Group&apos;), TEXT(Group_Type__c) == &apos;Tutor Group&apos;)  ),true,false
)</errorConditionFormula>
        <errorMessage>Group cannot be updated from or to group type &apos;Tutor Group&apos;. Please consider creating a new group and deleting this one or contact your system administrator for further assistance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Whole_School_Group_Edit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Is_Whole_School_Group__c = TRUE, $Profile.Name &lt;&gt; &quot;System Administrator&quot;, !ISCHANGED( Assessments__c ), !ISCHANGED( Active_Registrations__c ), !ISCHANGED( Chatter_Group_Id__c ))</errorConditionFormula>
        <errorMessage>Only the system administrator can edit this student group. Please contact them if you require any changes to be made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Year_Group_Lowest_vs_Highest</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    Year_Group_Lowest_Position_Score__c &gt;=  Year_Group_Highest_Position_Score__c,
    NOT(ISBLANK(Year_Group_Lowest_Position_Score__c)),
    NOT(ISBLANK(Year_Group_Highest_Position_Score__c))
    )</errorConditionFormula>
        <errorMessage>The &apos;Year Group (Highest - if any)&apos; cannot be lower or the same as &apos;Year Group (Lowest)&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_Ownership_Change</fullName>
        <active>true</active>
        <description>validate only group owner can change record ownership</description>
        <errorConditionFormula>IF(
   OR(	
      AND(
          PRIORVALUE(OwnerId) != OwnerId , 
          PRIORVALUE(OwnerId) != $User.Id, 
          $Profile.Name != &apos;System Administrator&apos;, 
          $Profile.Name != &apos;School IT&apos;, 
          $Profile.Name != &apos;Headteacher / of Admin / of Finance&apos;,
          $Profile.Name != &apos;Portal Liaison (Super User)&apos;,
          !ISBLANK(PRIORVALUE(OwnerId))
          ),
      AND(
          TEXT(Owner:User.School__c) != TEXT($User.School__c),
          $Profile.Name != &apos;System Administrator&apos;
          )
      ),
   TRUE, FALSE
   )</errorConditionFormula>
        <errorMessage>You don&apos;t have appropriate access to change group ownership or you&apos;re trying to change ownership to a teacher in another school.</errorMessage>
    </validationRules>
