    <fields>
        <fullName>Year_Group_Highest_Position_Score__c</fullName>
        <description>Used in a validation rule to ensure that the &apos;Year Group (Highest - if any)&apos; field is not the same as or less than the &apos;Year Group (Lowest)&apos; field.</description>
        <externalId>false</externalId>
        <formula>IF(
   ISPICKVAL(Combined_Year_Group__c, &quot;Pre-Nursery&quot;), 
   0.2,
   IF(
      ISPICKVAL(Combined_Year_Group__c, &quot;Nursery&quot;), 
      0.4,
      IF(
         OR(
            ISPICKVAL(Combined_Year_Group__c, &quot;Reception&quot;), 
            ISPICKVAL(Combined_Year_Group__c, &quot;Pre-Kindergarten&quot;)
            ),
         0.6,
         IF(
            ISPICKVAL(Combined_Year_Group__c, &quot;Kindergarten&quot;), 
            0.8,
            IF(
               OR(
                  ISPICKVAL(Combined_Year_Group__c, &quot;Year 10&quot;), 
                  ISPICKVAL(Combined_Year_Group__c, &quot;Grade 10&quot;)
                  ),
               10.0,
               IF(
                  OR(
                     ISPICKVAL(Combined_Year_Group__c, &quot;Year 11&quot;), 
                     ISPICKVAL(Combined_Year_Group__c, &quot;Grade 11&quot;)
                     ),
                  11.0,
                  IF(
                     OR(
                        ISPICKVAL(Combined_Year_Group__c, &quot;Year 12&quot;), 
                        ISPICKVAL(Combined_Year_Group__c, &quot;Grade 12&quot;)
                        ),
                     12.0,
                     IF(
                        ISPICKVAL(Combined_Year_Group__c, &quot;Year 13&quot;), 
                        13.0,
                        VALUE(RIGHT(TEXT(Combined_Year_Group__c), 1))
                        )
                     )
                  )
               )
            )
         )
      )
   )</formula>
        <label>Year Group (Highest) Position Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
