    <validationRules>
        <fullName>Disallow_Empty_Name_if_Use_in_Trolley</fullName>
        <active>true</active>
        <errorConditionFormula>AND (TEXT( Status__c ) = &quot;Used in Trolley&quot;,  Asset_Trolley_Name__c =&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Asset_Trolley_Name__c</errorDisplayField>
        <errorMessage>Trolley Name must be specified if the Asset is used in a trolley</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Asset_is_chosen</fullName>
        <active>true</active>
        <errorConditionFormula>School_Asset__c =&quot;&quot;</errorConditionFormula>
        <errorDisplayField>School_Asset__c</errorDisplayField>
        <errorMessage>You must associate one asset to this custody record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Return_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Date_Returned__c &lt; Date_Checked_Out__c</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Date Returned Cannot be BEFORE the Check-Out date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Return_Date_v_status</fullName>
        <active>true</active>
        <description>Ensures that a return date is inputed if a Custody is completed</description>
        <errorConditionFormula>AND( TEXT(Status__c )&lt;&gt;&quot;Used in Trolley&quot;, TEXT(Status__c )&lt;&gt;&quot;On Loan&quot;, TEXT(Status__c )&lt;&gt;&quot;Not returned&quot;, ISBLANK(Date_Returned__c))</errorConditionFormula>
        <errorDisplayField>Date_Returned__c</errorDisplayField>
        <errorMessage>Given the Status selected, you must input a Return Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_at_least_One_Custodian</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    Staff_Member__c =&quot;&quot;, 
    Student__c =&quot;&quot;,
    TEXT(Status__c) !=&quot;Used in Trolley&quot;,
    TEXT(PRIORVALUE(Status__c)) !=&quot;Used in Trolley&quot;
    )</errorConditionFormula>
        <errorMessage>You must select EITHER a staff member or student as custodian</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_both_custodians</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Staff_Member__c  &lt;&gt; &quot;&quot;, Student__c &lt;&gt;&quot;&quot;)</errorConditionFormula>
        <errorMessage>You must have EITHER one staff member OR one student. You can&apos;t have BOTH.</errorMessage>
    </validationRules>
