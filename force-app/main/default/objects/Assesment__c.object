    <fields>
        <fullName>Academ_Year_Manual_Override__c</fullName>
        <defaultValue>false</defaultValue>
        <description>To manually override the value of the Academic_Year__c field on the assessment record. In case of manual override the value from the Manual_Academ_Year__c field should be used.</description>
        <externalId>false</externalId>
        <label>Academic Year Manual Override</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Academic_Year__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   Academ_Year_Manual_Override__c = TRUE,
   Manual_Academ_Year__c,
   IF(
      OR(
         AND(
             MONTH(DATEVALUE(CreatedDate))=7, 
             DAY(DATEVALUE(CreatedDate))&gt;30
             ), 
         AND(
             MONTH(DATEVALUE(CreatedDate))&lt;=12, 
             MONTH(DATEVALUE(CreatedDate))&gt;=8
             )
        ), 
        TEXT(YEAR(DATEVALUE(CreatedDate)))&amp;&quot;/&quot;&amp; TEXT(YEAR(DATEVALUE(CreatedDate))+1), 
        TEXT(YEAR(DATEVALUE(CreatedDate ))-1)&amp;&quot;/&quot;&amp;TEXT(YEAR(DATEVALUE(CreatedDate)))
      )
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Academic Year</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
