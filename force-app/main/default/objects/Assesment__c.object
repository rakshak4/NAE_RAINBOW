IF(Mid_Term_Report__c =&quot;YES&quot;,&quot;0&quot;,&quot;1&quot;)&amp; 

CASE(TEXT(Type__c),&quot;Progress Report&quot;,&quot;1&quot;,&quot;Examination&quot;,&quot;1&quot;,&quot;0&quot;)&amp; 

CASE(Group__r.Group_Type__c,&quot;School Group&quot;,&quot;2&quot;,&quot;Tutor Group&quot;,&quot;1&quot;,&quot;0&quot;)&amp; 

Owner_Order_Copy__c &amp; 

Case(Group__r.Subject_Name__c, &quot;Personal, Social and Emotional Development&quot;,&quot;70&quot;, &quot;Communication and Language&quot;,&quot;65&quot;, &quot;Physical Development&quot;,&quot;60&quot;, &quot;English&quot;,&quot;55&quot;, &quot;Literacy&quot;,&quot;54&quot;, &quot;Reading&quot;,&quot;53&quot;, &quot;Writing&quot;,&quot;52&quot;, &quot;Mathematics&quot;,&quot;47&quot;, &quot;Mathematics - Statistics&quot;,&quot;46&quot;, &quot;Numeracy&quot;,&quot;45&quot;, &quot;Understanding the World&quot;,&quot;44&quot;, &quot;Expressive Arts &amp; Design&quot;,&quot;43&quot;, &quot;Science&quot;,&quot;42&quot;, &quot;Physics&quot;,&quot;41&quot;, &quot;Chemistry&quot;,&quot;40&quot;, &quot;Biology&quot;,&quot;39&quot;, &quot;History&quot;,&quot;38&quot;, &quot;Geography&quot;,&quot;37&quot;, &quot;Humanities&quot;,&quot;36&quot;, &quot;Integrated Curriculum&quot;,&quot;35&quot;, &quot;ICT&quot;,&quot;34&quot;, &quot;Computing&quot;,&quot;33&quot;, &quot;Computer Science&quot;,&quot;32&quot;, &quot;Business Studies&quot;,&quot;30&quot;, &quot;Media Studies&quot;,&quot;29&quot;, &quot;Economics&quot;,&quot;28&quot;, &quot;Myanmar&quot;,&quot;26&quot;, &quot;Russian&quot;,&quot;25&quot;, &quot;Chinese&quot;,&quot;24&quot;, &quot;Bahasa&quot;,&quot;23&quot;, &quot;Portuguese&quot;,&quot;22&quot;, &quot;Uzbek&quot;,&quot;21&quot;, &quot;French&quot;,&quot;20&quot;, &quot;Spanish&quot;,&quot;19&quot;, &quot;German&quot;,&quot;17&quot;, &quot;PSHE&quot;,&quot;16&quot;, &quot;Music&quot;,&quot;15&quot;, &quot;Drama&quot;,&quot;14&quot;, &quot;Dance&quot;,&quot;13&quot;, &quot;Art &amp; Design&quot;,&quot;12&quot;, &quot;Graphic Design&quot;,&quot;11&quot;, &quot;PE&quot;,&quot;10&quot;, &quot;Religious Education&quot;,&quot;09&quot;, &quot;Sociology&quot;,&quot;08&quot;, &quot;EAL&quot;,&quot;02&quot;, &quot;Learning Support&quot;,&quot;01&quot;, &quot;00&quot;)&amp; 

CASE(TEXT(Type__c),&quot;Progress Report&quot;,&quot;2&quot;,&quot;Examination&quot;,&quot;1&quot;,&quot;0&quot;)&amp; 

CASE(Grade_Type__c, &quot;Exam&quot;, &quot;5&quot;, &quot;Predicted Grade&quot;, &quot;4&quot;, &quot;Mock Exam&quot;, &quot;3&quot;, &quot;Target Grade&quot;, &quot;2&quot;, &quot;Current Working Grade&quot;, &quot;1&quot;, &quot;0&quot;)&amp; 

LEFT(RIGHT(TEXT(CreatedDate), 15), 2) &amp; 
LEFT(RIGHT(TEXT(CreatedDate), 12), 2) &amp; 
LEFT(RIGHT(TEXT(CreatedDate), 9), 2) &amp; 
LEFT(RIGHT(TEXT(CreatedDate), 6), 2) &amp; 
LEFT(RIGHT(TEXT(CreatedDate), 3), 2)</formula>
