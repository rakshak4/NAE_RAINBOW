    <validationRules>
        <fullName>Assesment_Record_and_Student</fullName>
        <active>true</active>
        <description>Ensuring an Assesment record always has a student attached</description>
        <errorConditionFormula>/*This contained an invalid RecordTypeId value in BSF SFDC and has only just been corrected. If any problems occur than it may be down to this increased validation */

AND(RecordTypeId  =&quot;0126F000001hVzJ&quot;, Student__c =&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Student__c</errorDisplayField>
        <errorMessage>An Assessment record must be linked to a student.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_Spacers</fullName>
        <active>true</active>
        <description>Prevents the spacers being used as an effort grade</description>
        <errorConditionFormula>OR(
   TEXT(Achievement__c) = &quot;------PRIMARY------&quot;,
   TEXT(Achievement__c) = &quot;----EYFS/PRIMARY----&quot;,
   TEXT(Effort__c) = &quot;------PRIMARY------&quot;,
   TEXT(Effort__c) = &quot;----EYFS/PRIMARY----&quot;,
   TEXT(Effort__c) = &quot;------SECONDARY------&quot;,
   TEXT(Effort__c) = &quot;--------------&quot;
   )</errorConditionFormula>
        <errorMessage>You cannot use a spacer in the &apos;Effort&apos; or &apos;Achievement&apos; field. Please select a valid option.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Grade_Length</fullName>
        <active>true</active>
        <description>The rule allows 4 characters length only in case the grade is &apos;100%&apos; or &apos;A*A*&apos;</description>
        <errorConditionFormula>AND(LEN(Grade__c) = 4, Grade__c != &quot;100%&quot;, Grade__c != &quot;A*A*&quot;)</errorConditionFormula>
        <errorDisplayField>Grade__c</errorDisplayField>
        <errorMessage>Grade may be 4 characters long only if it is &apos;100%&apos; or &apos;A*A*&apos;.
If you need another 4 character grade to be added then please contact your system administrator.</errorMessage>
    </validationRules>
