    <validationRules>
        <fullName>Check_Complete_before_rate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(Date_Completed__c)=&quot;&quot;,TEXT(Rate_Satisfaction__c)&lt;&gt;&quot;&quot;,TEXT(Rate_Satisfaction__c)&lt;&gt;&quot;Not Rated&quot;)</errorConditionFormula>
        <errorMessage>You cannot rate a task before a &quot;Completed Date&quot; is entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Checks_Category_when_Ebooks_are_added</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TEXT(Design_Category__c )&lt;&gt; &quot;Digital Library&quot;, Ebooks_Added__c &gt;0)</errorConditionFormula>
        <errorMessage>Select &quot;Digital Library&quot; as Design category when stating Ebooks have been added.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Checks_Contact_Details_filled_in</fullName>
        <active>true</active>
        <description>Checks_Contact_Details_filled_in when not Group Designer</description>
        <errorConditionFormula>AND(
    OR(
       Contact_Email__c = &quot;&quot;,
       Contact_Person__c = &quot;&quot;
       ),
    TEXT(Work_For_Override__c) != &quot;The British Schools Foundation&quot;
    )</errorConditionFormula>
        <errorMessage>Please enter a Contact Person and an Email Address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Checks_No_Rating_is_made_is_checked_No_R</fullName>
        <active>true</active>
        <description>Checks No Rating is made is checked No Rating required</description>
        <errorConditionFormula>AND( No_Rating_Required__c =TRUE, TEXT(Rate_Satisfaction__c )&lt;&gt;&quot;Not Rated&quot;)</errorConditionFormula>
        <errorMessage>A Satisfaction Rating cannot be made if the field &quot;No Rating Required&quot; is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Checks_Scope_of_Job_is_completed</fullName>
        <active>true</active>
        <description>Checks Scope of Job is completed if a rating is made</description>
        <errorConditionFormula>AND( TEXT( Scope_of_Job__c )=&quot;&quot;, TEXT(Rate_Satisfaction__c )&lt;&gt;&quot;Not Rated&quot;)</errorConditionFormula>
        <errorMessage>You must complete &quot;Scope of Job&quot; field, when entering a rating.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Checks_Work_For_is_assigned</fullName>
        <active>false</active>
        <errorConditionFormula>AND (CreatedBy.CompanyName =&quot;Group Designer&quot;, 



OR(TEXT( Work_For_Override__c ) =&quot;No Override&quot;,TEXT(Work_For_Override__c) =&quot;None&quot;,TEXT(Work_For_Override__c) =&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Abbreviation_Text__c</errorDisplayField>
        <errorMessage>If the record is created by the Group Designer, you must select a &quot;Work For (Override)&quot; option, to ensure the hours are properly assigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Checks_if_amount_of_hours_are_registered</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(Hours_of_Work__c  &lt;= 0,TEXT(Hours_of_Work__c)=&quot;&quot;), TEXT(Date_Completed__c)&lt;&gt;&quot;&quot;)</errorConditionFormula>
        <errorMessage>You must register the number of Hours work to complete, if a completion date is added.</errorMessage>
    </validationRules>
