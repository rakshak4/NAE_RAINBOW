    <validationRules>
        <fullName>Ensure_Accounts_Match</fullName>
        <active>true</active>
        <errorConditionFormula>Grant_Application__r.Parent_Record__r.Id != Student__r.AccountId</errorConditionFormula>
        <errorMessage>A student financial aid application record can only link to a master financial aid application record for the same family. Please suggest an alternate financial aid application or student.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Changes_After_Approved</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    School__c != &quot;The New York International School&quot;,
    $Profile.Name != &quot;System Administrator&quot;,
    Grant_Application__r.Record_is_Locked__c = TRUE,
    OR(
       ISNEW(),
       ISCHANGED(Tuition_Reduction__c),
       ISCHANGED(Lunch_Granted__c),
       ISCHANGED(Bus_Granted__c),
       ISCHANGED(Enrolment_Fees__c),
       ISCHANGED(CurrencyIsoCode),
       ISCHANGED(Other_Benefits__c)
       )
       /*
       ,
    OR(
       PRIORVALUE(Tuition_Reduction__c) != &quot;&quot;,
       PRIORVALUE(Lunch_Granted__c) != &quot;&quot;,
       PRIORVALUE(Bus_Granted__c) != &quot;&quot;,
       PRIORVALUE(Enrolment_Fees__c) != &quot;&quot;,
       PRIORVALUE(Other_Benefits__c) != &quot;&quot;
       )*/
    )</errorConditionFormula>
        <errorMessage>You cannot change the percentages granted, currency, or other benefits when the financial aid is already approved, nor can you add a new student financial aid application to an approved financial aid record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Changes_After_Approved_NY</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    School__c = &quot;The New York International School&quot;,
    $Profile.Name != &quot;System Administrator&quot;,
    Grant_Application__r.Record_is_Locked__c = TRUE,
    OR(
       ISNEW(),
       ISCHANGED(Tuition_Amount__c),
       ISCHANGED(Lunch_Amount__c),
       ISCHANGED(Bus_Amount__c),
       ISCHANGED(Enrolment_Fee_Amounts__c),
       ISCHANGED(CurrencyIsoCode),
       ISCHANGED(Tuition_Reduction__c),
       ISCHANGED(Lunch_Granted__c),
       ISCHANGED(Bus_Granted__c),
       ISCHANGED(Enrolment_Fees__c),
       ISCHANGED(Other_Benefits__c)
       )
    )</errorConditionFormula>
        <errorMessage>You cannot change the amount granted, currency, or other benefits when the financial aid is already approved, nor can you add a new student financial aid application to an approved financial aid record.</errorMessage>
    </validationRules>
