    <fields>
        <fullName>Grant_Start_Date_for_KPIs__c</fullName>
        <description>This will return the 01/08 of the academic year in which the financial aid started/starts if the start year of the financial aid is before or after the current one. Otherwise, if the start year is this academic year, it will calculate the grant start date according to the start term provided.</description>
        <externalId>false</externalId>
        <formula>IF(
   OR(
      VALUE(LEFT(Grant_Application__r.Start_Year__c, 4)) &lt; YEAR(Student__r.Related_School__r.First_Day_of_This_Fiscal_Year__c),
      VALUE(LEFT(Grant_Application__r.Start_Year__c, 4)) &gt; YEAR(Student__r.Related_School__r.First_Day_of_This_Fiscal_Year__c)
      ),
   DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4)),08,01),
   CASE(
        Grant_Application__r.Start_Term__c,
        &quot;January&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4))+1, 01,01),
        &quot;February&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4))+1, 02,01),
        &quot;March&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4))+1, 03,01),
        &quot;April&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4))+1, 04,01),
        &quot;May&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4))+1, 05,01),
        &quot;June&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4))+1, 06,01),
        &quot;July&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4))+1, 07,01),
        &quot;August&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4)), 08,01),
        &quot;September&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4)), 09,01),
        &quot;October&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4)), 10,01),
        &quot;November&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4)), 11,01),
        &quot;December&quot;, DATE(VALUE(LEFT(Grant_Application__r.Start_Year__c, 4)), 12,01),
        &quot;Term 1&quot;, Student__r.Related_School__r.Start_Date_T1_TY__c,
        &quot;Term 2&quot;, Student__r.Related_School__r.Start_Date_T2_TY__c,
        &quot;Term 3&quot;, Student__r.Related_School__r.Start_Date_T3_TY__c,
        null
        )
   )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Grant Start Date (for KPIs)</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
