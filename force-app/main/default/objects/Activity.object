   LEN(TEXT(MONTH(ActivityDate)))=1,
   &quot;0&quot;&amp;TEXT(MONTH(ActivityDate)),
   TEXT(MONTH(ActivityDate))
   )

&amp;&quot;-&quot;&amp;

IF(
   LEN(TEXT(DAY(ActivityDate)))=1,
