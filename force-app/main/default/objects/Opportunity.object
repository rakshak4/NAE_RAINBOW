        <fullName>On_Roll_Change_to_Kompanion__c</fullName>
        <description>Result checked daily, by means of a batch job, to trigger date related updates in School Kompanion where formula evaluates to TRUE</description>
        <externalId>false</externalId>
        <formula>School_Kompanion_On_Roll__c !=

IF(
   OR( 
      Kompanion_Keep_On_Roll_Formula__c = TRUE, 
      AND(
          Make_Off_Roll_In_Kompanion__c != TRUE,
          CloseDate - 14 &lt;= TODAY()-1,
          CloseDate &gt; TODAY()-1,
          OR(
             AND(
                 TEXT(Add_to_Register__c)=&quot;Yes&quot;, 
                 TEXT(StageName) = &quot;A/ Deposit&quot;
                 ),
             TEXT(StageName) = &quot;Enrolled&quot;
             )
          )
      ),
   &quot;Yes&quot;,
   IF(
      AND(
          Make_Off_Roll_In_Kompanion__c != TRUE,
          CloseDate &lt;= TODAY()-1,
          OR(
             AND(
                 TEXT(Add_to_Register__c)=&quot;Yes&quot;, 
                 TEXT(StageName) = &quot;A/ Deposit&quot;
                 ),
             TEXT(StageName) = &quot;Enrolled&quot;
             )
          ),
      &quot;Yes&quot;,
      IF(
         OR(
            Make_Off_Roll_In_Kompanion__c = TRUE,
            TEXT(StageName) = &quot;Left&quot;,
            AND(
                Has_Been_Enrolled__c = TRUE,
                School_Kompanion_Student_Created__c = TRUE
                )
            ),
         &quot;No&quot;,
         &quot;Doesn&apos;t Qualify&quot;
         )
      )
   )</formula>
        <label>On Roll Change to Kompanion</label>
