        <externalId>false</externalId>
        <formula>IF(
   AND(
       TODAY() &lt; DATE(2022, 8 , 1), 
       OR(
          School__c = &quot;The British School of Navarra&quot;,
          /*School__c = &quot;The British International School of Marbella&quot;,*/
          CONTAINS(School__c, &quot;Marbella&quot;)
          )
       ), 
   DATE(2022, 1 , 1), 
   IF(
      AND(
          TODAY() &lt; DATE(2022, 8 , 1),  
          OR(
             School__c = &quot;The British College of Brazil&quot;,
             School__c = &quot;The British School of Brasilia&quot;
             )
          ), 
      DATE(2021, 7 , 1), 
      IF(
         AND(
             TODAY() &gt;= DATE(2022, 8 , 1), 
             TODAY() &lt; DATE(2023, 8 , 1)
             ), 
         DATE(2022, 9, 1), 
         DATE(2021, 9, 1)
         )
      )
   )</formula>
