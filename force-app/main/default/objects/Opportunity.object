    <validationRules>
        <fullName>Actual_Year_Group_not_Not_Sure</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISPICKVAL(Year_Group__c, &quot;Not Sure&quot;), ISPICKVAL(Year_Group__c, &quot;&quot;))</errorConditionFormula>
        <errorMessage>A student Birth Date and Starting Date combination results in an invalid year group. Please correct the error before continuing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BSKL_Prevent_Non_Nurse_Edit</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
    ISCHANGED(Special_Considerations_Detail__c),
    School__c = &quot;The British International School of Kuala Lumpur&quot;,   
    $UserRole.Id != &quot;00E6F000001TY00&quot;,
    $Profile.Id != &quot;00e6F000001hU3F&quot;,
    OR(
       TEXT(StageName) = &quot;Enrolled&quot;,
       AND(
           TEXT(StageName) = &quot;A/ Deposit&quot;,
           TEXT(Add_to_Register__c) = &quot;Yes&quot;
           )
       ) 
    )</errorConditionFormula>
        <errorMessage>Please contact the school nurse if you wish to change the contents of the &apos;Special Considerations Detail&apos; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Custom_FTE_Value_Reason_Required</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
   AND(
       ISBLANK(Custom_FTE_Value_Details__c),
       NOT(ISBLANK(Custom_FTE_Value__c))
       ),
