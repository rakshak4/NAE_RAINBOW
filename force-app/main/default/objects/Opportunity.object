    <webLinks>
        <fullName>Send_To_Kompanion</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send To Kompanion</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 

var doProcess = &quot;{!Opportunity.Qualifies_for_SK_Outbound_Message__c}&quot;; 
if(doProcess != &quot;1&quot;){ 
alert(&quot;Student does not meet criteria for sending to Kompanion.&quot;); 
}else{ 
var newRecords = []; 

var opportunity = new sforce.SObject(&quot;Opportunity&quot;); 
opportunity.id =&quot;{!Opportunity.Id}&quot;; 
opportunity.Trigger_Batch_SendKompanionOutbound__c= &quot;TRUE&quot;; 
newRecords.push(opportunity); 

var result = sforce.connection.update(newRecords); 

for (var i=0; i&lt;result.length; i++) { 
if (result[i].getBoolean(&quot;success&quot;)) { 
alert(&quot;Request was submitted successfully. You should receive an email only if any error occurs in the process.&quot;); 
window.location.reload(); 
} else { 
var errorMessage = &quot;Failed to process request. You likely do not have the permissions required to successfully send this student to Kompanion.&quot;; 
errorMessage += &quot;&lt;br/&gt;&quot;; 
errorMessage += &quot;Complete Error Details:&quot; 
errorMessage += &quot;&lt;br/&gt;&quot;; 
errorMessage += result[i]; 
alert(errorMessage); 
} 
} 
}</url>
