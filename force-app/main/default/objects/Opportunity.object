          NOT(CONTAINS(Class_Copy__c, &quot;Pending&quot;))
          ),
      &quot; &quot;&amp; House_Copy__c &amp; RIGHT(Class_Copy__c, 2),
      &quot; &quot;&amp; Class_Copy__c
      ),
   IF(
      AND(
          LEN(Class_Next_Year_Copy__c)&gt;1,
          NOT(CONTAINS(Class_Next_Year_Copy__c, &quot;Pending&quot;))
          ),
      &quot; &quot;&amp; House_Next_Year_Copy__c &amp; RIGHT(Class_Next_Year_Copy__c, 2),
