        <fullName>Has_Discount_Inconsistency__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   OR( 
      AND( 
          Financial_Aid_Note__c = &quot;Review details in Financial Aid section below&quot;, 
          Has_Current_Discounted_Fee__c = 0 
          ), 
      AND(
          Financial_Aid_Note__c = &quot;None Approved&quot;, 
          Has_Current_Discounted_Fee__c &gt;= 1 
          ) 
      ), 
   &quot;TRUE&quot;, 
   &quot;FALSE&quot;
   )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Has Discount Inconsistency?</label>
