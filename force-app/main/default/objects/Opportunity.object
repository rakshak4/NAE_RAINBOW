        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Has_EAL_Inconsistency__c</fullName>
        <externalId>false</externalId>
        <formula>OR( 
   AND(
       Has_Current_EAL_Fee__c &gt;= 1, 
       OR( 
          ISBLANK(EAL_Start_Date__c),  
          EAL_End_Date__c &lt; TODAY(),
          TEXT(SK_Additional_Learning_1__c)!=&quot;EAL&quot;,
          NOT(INCLUDES(Learning_Support_Provided__c, &quot;EAL - Gold&quot;))
          ) 
       ), 
   AND(
       Has_Current_EAL_Fee__c = 0, 
       OR(
          AND(
              NOT(ISBLANK(EAL_Start_Date__c)), 
              OR(
                 EAL_End_Date__c &gt; TODAY()+180, 
                 ISBLANK(EAL_End_Date__c)
                 )
              ),
          AND(
              NOT(ISBLANK(EAL_Start_Date__c)),
              NOT(ISBLANK(EAL_End_Date__c)),
              EAL_End_Date__c &lt; TODAY(),
              OR(
                 TEXT(SK_Additional_Learning_1__c)=&quot;EAL&quot;,
                 INCLUDES(Learning_Support_Provided__c, &quot;EAL - Gold&quot;)
                 )
              )
          ) 
       ),
   AND(
       TEXT(SK_Additional_Learning_1__c)=&quot;EAL&quot;,
       OR(
          NOT(INCLUDES(Learning_Support_Provided__c, &quot;EAL - Gold&quot;)),
          ISBLANK(EAL_Start_Date__c)
          )
       ),
   AND(
       INCLUDES(Learning_Support_Provided__c, &quot;EAL - Gold&quot;),
       OR(
          TEXT(SK_Additional_Learning_1__c)!=&quot;EAL&quot;,
          ISBLANK(EAL_Start_Date__c)
          )
       ),
   AND(
       NOT(ISBLANK(EAL_Start_Date__c)),
       EAL_Start_Date__c &lt;= TODAY(),
       EAL_End_Date__c &gt; TODAY(),
       OR(
          NOT(INCLUDES(Learning_Support_Provided__c, &quot;EAL - Gold&quot;)),
          TEXT(SK_Additional_Learning_1__c)!=&quot;EAL&quot;
          )
       )
   )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Has EAL Inconsistency?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
