    <fields>
        <fullName>Application_Period_Classification__c</fullName>
        <description>Classifies applications for NAE schools into the relevant period for use in a KPI dashboard component. The error values in the formula should never show on the dashboard as it would mean something strange has taken place e.g. a new student record created this week but with a starting date in a prior academic year.</description>
        <externalId>false</externalId>
        <formula>IF(
   AND(
       TODAY() &gt;= DATE(YEAR(Related_School__r.First_Day_of_This_Fiscal_Year__c),08,01),
       TODAY() &lt; Related_School__r.First_Day_of_This_Fiscal_Year__c
       ),
   IF(
      CloseDate &lt; Related_School__r.Start_Date_T1_TY__c - 14,
      &quot;1. For Current School Year&quot;,
      IF(
         AND(
             CloseDate &gt;= Related_School__r.Start_Date_T1_TY__c - 14,
             CloseDate &lt;= DATE(YEAR(Related_School__r.Start_Date_T1_TY__c),09,30)
             ),
         &quot;2. For September Next Year&quot;,
         IF(
            CloseDate &gt; DATE(YEAR(Related_School__r.Start_Date_T1_TY__c),09,30),
            &quot;3. For beyond September Next Year&quot;,
            &quot;ERROR - Prior School Year&quot;
            )
         )
      ),
   IF(
      AND(
          TODAY() &lt; DATE(YEAR(Related_School__r.First_Day_of_This_Fiscal_Year__c)+1,08,01),
          TODAY() &gt;= Related_School__r.First_Day_of_This_Fiscal_Year__c
          ),
      IF(
         AND(
             CloseDate &gt;= Related_School__r.Start_Date_T1_TY__c - 14,
             CloseDate &lt; Related_School__r.Start_Date_T1_NY__c - 14
             ),
         &quot;1. For Current School Year&quot;,
         IF(
            AND(
                CloseDate &gt;= Related_School__r.Start_Date_T1_NY__c - 14,
                CloseDate &lt;= DATE(YEAR(Related_School__r.Start_Date_T1_NY__c),09,30)
                ),
            &quot;2. For September Next Year&quot;,
            IF(
               CloseDate &gt; DATE(YEAR(Related_School__r.Start_Date_T1_NY__c),09,30),
               &quot;3. For beyond September Next Year&quot;,
               &quot;ERROR - Prior School Year&quot;
               )
            )
         ),
      &quot;ERROR - UNKNOWN&quot;
      )
   )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Application Period Classification</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
