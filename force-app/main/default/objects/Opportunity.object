    <fields>
        <fullName>School_Kompanion_On_Roll__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   OR(
      Kompanion_Keep_On_Roll_Formula__c = TRUE,
      AND(
          Make_Off_Roll_In_Kompanion__c != TRUE,
          CloseDate - 14 &lt;= TODAY(),
          CloseDate &gt; TODAY(),
          OR(
             AND(
                 TEXT(Add_to_Register__c)=&quot;Yes&quot;, 
                 TEXT(StageName) = &quot;A/ Deposit&quot;
                 ),
             TEXT(StageName) = &quot;Enrolled&quot;
             )
          )
      ),
   &quot;Yes&quot;,
   IF(
      AND(
          Make_Off_Roll_In_Kompanion__c != TRUE,
          CloseDate &lt;= TODAY(),
          OR(
             AND(
                 TEXT(Add_to_Register__c)=&quot;Yes&quot;, 
                 TEXT(StageName) = &quot;A/ Deposit&quot;
                 ),
             TEXT(StageName) = &quot;Enrolled&quot;
             )
          ),
      &quot;Yes&quot;,
      IF(
         OR(
            Make_Off_Roll_In_Kompanion__c = TRUE,
            TEXT(StageName) = &quot;Left&quot;,
            AND(
                Has_Been_Enrolled__c = TRUE,
                School_Kompanion_Student_Created__c = TRUE
                )
            ),
         &quot;No&quot;,
         &quot;Doesn&apos;t Qualify&quot;
         )
      )
   )</formula>
        <label>School Kompanion On Roll?</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
