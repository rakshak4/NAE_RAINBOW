    <validationRules>
        <fullName>End_Date_not_Earlier_than_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c &lt; Start_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>The End Date cannot be earlier than the Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fill_Contract_Term</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TEXT(Type__c) =&quot;Contract&quot;, TEXT(Contract_Term__c) =&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Contract_Term__c</errorDisplayField>
        <errorMessage>You must choose a contract Term</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Format_Academic_Year</fullName>
        <active>true</active>
        <errorConditionFormula>IF(LEN( Academic_Year__c ) = 7, FALSE, TRUE)</errorConditionFormula>
        <errorDisplayField>Academic_Year__c</errorDisplayField>
        <errorMessage>Format for Academic Year should be YYYY/YY. For example 2012/13</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Probation_Period_inputed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(Months_Probation__c) =&quot;None&quot;, TEXT(Type__c )=&quot;Contract&quot;)</errorConditionFormula>
        <errorDisplayField>Months_Probation__c</errorDisplayField>
        <errorMessage>You must indicate a probation period</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
    $User.Id != &quot;0056F000008tfhZ&quot;, 
    ISBLANK(End_Date__c), 
    OR(
       TEXT(Contract_Term__c) = &quot;Less than a year&quot;,
       TEXT(Contract_Term__c)= &quot;1 Year&quot;,
       TEXT(Contract_Term__c) = &quot;2 Years&quot;,
       TEXT(Contract_Term__c) = &quot;3 Years&quot;
       )
    )</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Fixed Term Contracts must have an End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Secondary_Currency_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
   AND(
       TEXT(Secondary_Currency__c) &lt;&gt; &quot;&quot;, 
       ISBLANK(Secondary_Currency_Applies_To__c)
       ),
   AND(TEXT(Secondary_Currency__c) = &quot;&quot;, 
       NOT(ISBLANK(Secondary_Currency_Applies_To__c))
       )
   )</errorConditionFormula>
        <errorMessage>If you select a secondary currency you must also select which salary component(s) it applies to and vice versa.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Termination_Date_less_than_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( TEXT ( Termination_Date__c ) &lt;&gt; &quot;&quot; , Termination_Date__c &gt; End_Date__c )</errorConditionFormula>
        <errorMessage>The termination date cannot be after the end date of the contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Termination_Date_not_allowed_for_status</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( TEXT ( Termination_Date__c ) &lt;&gt; &quot;&quot; , OR ( TEXT ( Status__c ) = &quot;Draft&quot;, TEXT ( Status__c ) = &quot;Awaiting Approval&quot;, TEXT ( Status__c ) = &quot;Approved-Not Signed&quot;, TEXT ( Status__c ) = &quot;Not signed&quot; ) )</errorConditionFormula>
        <errorMessage>You cannot enter a termination date for an agreement that has not been signed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Updates_should_not_have_End_Dates</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(Type__c) =&quot;Conditions Update&quot;, TEXT(End_Date__c )&lt;&gt;&quot;&quot;,TEXT( Status__c) &lt;&gt;&quot;Expired&quot; )</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Conditions Updates should not have an End Date. Use the Termination Date field to expire this record.</errorMessage>
    </validationRules>
