    <validationRules>
        <fullName>Ensure_Valid_Enquiry_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    ISNEW(),
    RecordType.DeveloperName = &quot;School_Tour&quot;,
    OR(
       TEXT(Related_Enquiry__r.Status) = &quot;New&quot;,
       TEXT(Related_Enquiry__r.Status) = &quot;Unqualified&quot;
       )
    )</errorConditionFormula>
        <errorMessage>You cannot log a school visit against an enquiry with an Enquiry Status of &apos;New&apos; or &apos;Unqualified&apos;. Please mark the enquiry as &apos;Qualified&apos; first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Valid_Related_Enquiry_Value</fullName>
        <active>true</active>
        <description>Required to ensure that a school visit record is connected to a</description>
        <errorConditionFormula>OR(
   AND(
       ISNEW(),
       RecordType.DeveloperName = &quot;School_Tour&quot;,
       ISBLANK(Related_Enquiry__c)
       ),
   AND(
       ISCHANGED(Related_Enquiry__c),
       NOT(ISBLANK(PRIORVALUE(Related_Enquiry__c)))
       )
   )</errorConditionFormula>
        <errorMessage>You cannot change the &apos;Related Enquiry&apos; nor can it be blank</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Mark_Visit_as_Complete</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mark Visit as Complete</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}

var Now = new Date(); 
var NowDateTime = Now.toISOString();

var myDate = &quot;{!Event.Event_Start_Date_Time__c}&quot;; 
var EventStartDate = new Date(myDate); 
var EventStartDateTime = EventStartDate.toISOString(); 

if(EventStartDateTime &gt; NowDateTime){
alert(&quot;You cannot mark a school visit as complete if it is scheduled in the future.&quot;);
}

else if({!Event.Repeat_Visit__c})
{var confirmResult = window.confirm(&quot;This record is marked as a repeat school visit so will not be included in your KPI totals. Once it&apos;s marked as complete you will no longer be able to edit it. Do you wish to continue?&quot;);
if(confirmResult){
var records = [];
var record = new sforce.SObject(&quot;event&quot;);
record.Id = &quot;{!Event.Id}&quot;;
record.RecordTypeId = &quot;0126F000001iC4P&quot;;
/* testing */ record.Test_1__c = NowDateTime;
/* testing */ record.Test_2__c = EventStartDateTime; 
records.push(record);
var result = sforce.connection.update(records);
//alert(&quot;2:result=&quot;+result);
var sb = &quot;&quot;;
for (var i=0; i&lt;result.length; i++) {
if (result[i].getBoolean(&quot;success&quot;)) {
sb += &quot;\nSuccess: This School Visit has been marked as complete.&quot;;
} else {
sb += &quot;\n Error. &quot; + result[i];
}
}
if(sb != &quot;&quot;){
alert(sb);

}
location.reload();	

}
}


else{
var confirmResult = window.confirm(&quot;By clicking this button you are confirming that you will log a school visit for {!Event.Number_of_Children_in_Enquiry__c} child(ren) in the current week&apos;s KPIs. Once this is done you will no longer be able to edit it. Do you wish to continue?&quot;);
if(confirmResult){
var records = [];
var record = new sforce.SObject(&quot;event&quot;);
record.Id = &quot;{!Event.Id}&quot;;
record.RecordTypeId = &quot;0126F000001iC4P&quot;;
record.School_Visit_Date_for_KPIs__c = new Date().toISOString();
records.push(record);
var result = sforce.connection.update(records);
//alert(&quot;2:result=&quot;+result);
var sb = &quot;&quot;;
for (var i=0; i&lt;result.length; i++) {
if (result[i].getBoolean(&quot;success&quot;)) {
sb += &quot;\nSuccess: This School Visit has been marked as complete.&quot;;
} else {
sb += &quot;\nError. You cannot mark this school visit as complete because another complete school visit already exists against the same enquiry. Please tick the &apos;Repeat Visit?&apos; checkbox to mark this as a repeat school visit and then try again.&quot;;
/*sb += &quot;\n Error. &quot; + result[i];*/
}
}
if(sb != &quot;&quot;){
alert(sb);

}
location.reload();	

}
}</url>
