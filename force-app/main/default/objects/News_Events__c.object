    <validationRules>
        <fullName>Disallow_Change_of_Date_to_Past</fullName>
        <active>true</active>
        <description>Prevents a user from editing an event to set new dates in the past</description>
        <errorConditionFormula>AND(
    RecordType.Name != &quot;News&quot;, 
    OR(
       AND(
           ISCHANGED(Event_Start_Date__c), 
           PRIORVALUE(Event_Start_Date__c) &gt;= TODAY(), 
           Event_Start_Date__c &lt; TODAY(),
           OR(
              Event_End_Date__c &lt; TODAY(),
              ISNULL(Event_End_Date__c)
              )
           ),
       AND(
           ISCHANGED(Event_End_Date__c), 
           PRIORVALUE(Event_End_Date__c) &gt;= TODAY(), 
           Event_End_Date__c &lt; TODAY()
           )
       )
    )</errorConditionFormula>
        <errorMessage>You cannot change the dates of an existing event to fall in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_Change_of_Record_Type</fullName>
        <active>true</active>
        <description>Prevents a record type being changed so that there are no issues with the website cron job/imports</description>
        <errorConditionFormula>ISCHANGED(RecordTypeId)</errorConditionFormula>
        <errorMessage>You cannot change the record type</errorMessage>
    </validationRules>
