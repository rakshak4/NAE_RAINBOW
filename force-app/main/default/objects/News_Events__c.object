           Main_Text__c = &quot;&quot;,
           English_Parent__r.Main_Text__c != &quot;&quot;
           ),
       AND(
           Meta_Tag_Title__c = &quot;&quot;,
           English_Parent__r.Meta_Tag_Title__c != &quot;&quot;
