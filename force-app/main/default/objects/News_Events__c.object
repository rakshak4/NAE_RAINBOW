    OR(
       ISCHANGED(Name),
       ISCHANGED(Subtitle__c),
       ISCHANGED(Date__c),
       ISCHANGED(Event_Start_Date__c),
       ISCHANGED(Event_End_Date__c),
       ISCHANGED(Publish_in_Portal_School_Hub__c),
       ISCHANGED(Term_Date_Identifier__c),
       ISCHANGED(Term__c),
       ISCHANGED(Language__c),
       ISCHANGED(Student_Group__c),
       ISCHANGED(English_Parent__c),
       ISCHANGED(Disable_All_Email_Notifications__c)
       )
