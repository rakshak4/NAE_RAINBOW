       INCLUDES(English_Parent__r.School_Override_Field__c, &quot;Navarra&quot;),
       INCLUDES(English_Parent__r.School_Override_Field__c, &quot;Brasilia&quot;),
       INCLUDES(English_Parent__r.School_Override_Field__c, &quot;New York&quot;),
       INCLUDES(English_Parent__r.School_Override_Field__c, &quot;Group Site&quot;),
       INCLUDES(English_Parent__r.School_Override_Field__c, &quot;Sandboxes&quot;),
       TEXT(School_Text__c) = &quot;The British International School of Marbella&quot;,
       TEXT(School_Text__c) = &quot;The British School of Navarra&quot;,
       TEXT(School_Text__c) = &quot;The British School of Brasilia&quot;,
       TEXT(School_Text__c) = &quot;The New York International School&quot;,
       TEXT(School_Text__c) = &quot;The British Schools Foundation&quot;
       )
    )</errorConditionFormula>
        <errorMessage>You do not have permission to create or edit a record for this school. Please contact your system administrator if you think you should have access.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_No_Override_with_Other_Schools</fullName>
        <active>true</active>
