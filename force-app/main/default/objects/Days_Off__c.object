    <fields>
        <fullName>Days_Off_Month__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   MONTH(Start_Date__c) = 08, &quot;August&quot;,
   IF(
      MONTH(Start_Date__c) = 09, &quot;September&quot;,
      IF(
         MONTH(Start_Date__c) = 10, &quot;October&quot;,
         IF(
            MONTH(Start_Date__c) = 11, &quot;November&quot;,
            IF(
               MONTH(Start_Date__c) = 12, &quot;December&quot;,
               IF(
                  MONTH(Start_Date__c) = 01, &quot;January&quot;,
                  IF(
                     MONTH(Start_Date__c) = 02, &quot;February&quot;,
                     IF(
                        MONTH(Start_Date__c) = 03, &quot;March&quot;,
                        IF(
                           MONTH(Start_Date__c) = 04, &quot;April&quot;,
                           IF(
                              MONTH(Start_Date__c) = 05, &quot;May&quot;,
                              IF(
                                 MONTH(Start_Date__c) = 06, &quot;June&quot;,
                                 IF(
                                    MONTH(Start_Date__c) = 07, &quot;July&quot;,
    &quot;&quot;
  ))))))))))))</formula>
        <label>Days Off Month</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
