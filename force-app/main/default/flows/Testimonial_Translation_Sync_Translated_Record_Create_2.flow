            </value>
        </processMetadataValues>
        <name>formula_myRule_1_pmetrule</name>
        <dataType>Boolean</dataType>
        <expression>AND(CONTAINS({!myVariable_old.RecordType.DeveloperName}, &quot;Translation&quot;), {!myVariable_old.Newly_Created__c} = FALSE)</expression>
    </formulas>
